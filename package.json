{
	"name": "discord-bot",
	"version": "0.0.0",
	"description": "",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "pnpm recursive --include-workspace-root run build && node --env-file=.env dist/index.js | pino-pretty",
		"build": "node esbuild.js",
		"lint": "biome check --write ./src",
		"tsc": "tsc --noEmit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release": "pnpm recursive run build && changeset publish"
	},
	"packageManager": "pnpm@10.22.0",
	"dependencies": {
		"@ai-sdk/openai": "^2.0.65",
		"@kubernetes/client-node": "^1.4.0",
		"@sentry/node": "^10.25.0",
		"ai": "^5.0.92",
		"bufferutil": "^4.0.9",
		"discord-player": "^7.2.0-dev.2",
		"discord-player-googlevideo": "workspace:*",
		"discord-player-spotify": "^1.1.7",
		"discord.js": "^14.24.2",
		"ioredis": "^5.8.2",
		"pino": "^10.1.0",
		"sodium-native": "^5.0.9",
		"ulid": "^3.0.1",
		"zlib-sync": "^0.1.10",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@changesets/cli": "^2.29.7",
		"@discordjs/rest": "^2.6.0",
		"@sentry/esbuild-plugin": "^4.6.0",
		"@types/node": "catalog:",
		"@vitest/coverage-v8": "catalog:",
		"date-fns": "^4.1.0",
		"esbuild": "^0.27.0",
		"esbuild-plugin-pino": "2.3.3",
		"fuse.js": "^7.1.0",
		"memoize": "^10.2.0",
		"p-debounce": "^5.1.0",
		"p-queue": "^9.0.0",
		"pino-pretty": "^13.1.2",
		"pretty-bytes": "^7.1.0",
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"pnpm": {
		"overrides": {
			"libsodium-wrappers": "-"
		}
	}
}
