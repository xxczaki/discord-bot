		"memoize": "^10.1.0",
		"p-debounce": "^4.0.0",
		"p-queue": "^8.1.1",
		"pino-pretty": "^13.1.1",
		"pretty-bytes": "^7.1.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"overrides": {
			"libsodium-wrappers": "-"
		},
		"onlyBuiltDependencies": [
			"esbuild",
			"@sentry/cli",
			"@biomejs/biome",
			"bufferutil",
			"zlib-sync"
		]
	}
