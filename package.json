		"bufferutil": "^4.0.9",
		"discord-player": "^7.2.0-dev.2",
		"discord-player-spotify": "^1.1.7",
		"discord-player-youtubei": "^1.5.0",
		"discord.js": "^14.21.0",
		"ioredis": "^5.6.1",
		"pino": "^9.7.0",
		"sodium-native": "^5.0.6",
		"ulid": "^3.0.1",
		"zlib-sync": "^0.1.10"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@discordjs/rest": "^2.5.1",
		"@sentry/esbuild-plugin": "^3.6.1",
		"@types/node": "^24.1.0",
		"@vitest/coverage-v8": "3.2.4",
		"date-fns": "^4.1.0",
		"esbuild": "^0.25.8",
		"esbuild-plugin-pino": "^2.2.2",
		"fuse.js": "^7.1.0",
		"memoize": "^10.1.0",
		"p-debounce": "^4.0.0",
		"p-queue": "^8.1.0",
		"pino-pretty": "^13.0.0",
		"pretty-bytes": "^7.0.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"overrides": {
			"libsodium-wrappers": "-",
			"youtubei.js": "github:LuanRT/YouTube.js"
		},
		"onlyBuiltDependencies": [
			"esbuild",
			"@sentry/cli",
			"@biomejs/biome",
			"bufferutil",
			"zlib-sync"
		]
	}
