		"bufferutil": "^4.0.9",
		"discord-player": "^7.2.0-dev.2",
		"discord-player-spotify": "^1.1.7",
		"discord-player-youtubei": "^1.5.0",
		"discord.js": "^14.22.1",
		"ioredis": "^5.8.0",
		"pino": "^9.9.5",
		"sodium-native": "^5.0.9",
		"ulid": "^3.0.1",
		"zlib-sync": "^0.1.10"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@discordjs/rest": "^2.6.0",
		"@sentry/esbuild-plugin": "^4.3.0",
		"@types/node": "^24.5.2",
		"@vitest/coverage-v8": "3.2.4",
		"date-fns": "^4.1.0",
